  "license": "ISC",
  "homepage": "http://kvs.io",
  "dependencies": {
    "LiveScript": "^1.3.1",
    "basho-riak-client": "^1.1.1",
    "bunyan": "^1.3.5",
    "bunyan-prettystream": "^0.1.3",
    "dtrace-provider": "^0.4.0",
    "ipv6": "^3.1.1",
    "ipware": "0.0.5",
    "restify": "^3.0.2"
  },
  "devDependencies": {
    "async": "^0.9.0",
    "chai": "^2.2.0",
    "mocha": "^2.2.1",
    "nodemon": "1.3.0-6",
    "sinon": "^1.14.1"
  },
  "repository": {
    "type": "git",
