{
  "name": "kvs.io",
  "version": "0.0.1",
  "description": "kvs.io",
  "main": "api.ls",
  "scripts": {
    "start": "lsc api.ls",
    "test": "mocha --reporter spec --compilers ls:LiveScript --growl test/test-*ls",
    "start_mon": "nodemon -L -e ls --exec lsc api.ls",
    "test_mon": "nodemon -L -e ls --exec 'node_modules/mocha/bin/mocha --reporter spec --compilers ls:LiveScript --growl test/test-*ls || growlnotify --message \"Nothing passed.\" --image node_modules/mocha/images/error.png Crashed'"
  },
  "bin": {
